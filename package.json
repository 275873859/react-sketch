{
  "name": "react-sketch",
  "version": "0.2.1",
  "description": "Sketch Element for React based applications, backed-up by fabricjs as its core",
  "keywords": [
    "react",
    "canvas",
    "sketch",
    "fabricjs",
    "fabric.js"
  ],
  "author": {
    "name": "Thomas Bolis",
    "email": "thomas.bolis@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tbolis/react-sketch.git"
  },
  "bugs": {
    "url": "https://github.com/tbolis/react-sketch/issues"
  },
  "license": "MIT",
  "main": "./lib",
  "scripts": {
    "clean": "rimraf lib && rimraf build",
    "prebuild": "rimraf lib",
    "build": "webpack --config webpack.library.cfg.js --display-modules",
    "build:light": "babel ./src --out-dir ./lib",
    "build:examples": "webpack --config webpack.examples.cfg.js",
    "prepublish": "npm run test && npm run build",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "start": "node webpack.server.js",
    "test": "karma start karma.config.js",
    "posttest": "npm run lint"
  },
  "peerDependencies": {
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  },
  "devDependencies": {
    "babel": "^5.0.0",
    "babel-core": "^5.0.0",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^5.0.0",
    "canvas": "^1.3.12",
    "chai": "^3.4.1",
    "core-js": "^2.0.3",
    "css-loader": "^0.23.1",
    "eslint": "^1.2.1",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-react": "^3.3.0",
    "fabric": "^1.5.0",
    "flexboxgrid": "^6.3.0",
    "html-webpack-plugin": "^1.7.0",
    "karma": "^0.13.19",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^0.5.3",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.5",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-phantomjs-shim": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "material-ui": "^0.14.2",
    "mocha": "^2.3.4",
    "open": "0.0.5",
    "open-browser-webpack-plugin": "0.0.2",
    "phantomjs": "^1.9.19",
    "phantomjs-prebuilt": "^2.1.3",
    "react": "latest",
    "react-addons-test-utils": "^0.14.6",
    "react-color": "^1.3.6",
    "react-dom": "latest",
    "react-hot-loader": "^1.3.0",
    "react-tap-event-plugin": "latest",
    "rimraf": "^2.5.1",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.11",
    "webpack-dev-server": "^1.14.1"
  }
}
